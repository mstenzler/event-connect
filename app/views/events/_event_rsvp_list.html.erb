<% event.rsvps.each do |rsvp| %>
  <div class="row">
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= rsvp.user.display_name %></span>
          <p>Code: <%= rsvp.code %>
          </p>
        </div>
        <div class="card-action">
          <% if rsvp.user_id == current_user.id %>
            <button class="event-unrsvp" data-event-id="<%= event.id %>" data-rsvp-id="<%= rsvp.id %>">UnRsvp</button>
            <% end %>
            <button>Like</button>
        </div>
      </div>
    </div>
  </div>
<% end %>