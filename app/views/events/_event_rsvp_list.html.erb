<% event.rsvps.each do |rsvp| %>
  <div class="row">
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= rsvp.user.display_name %></span>
          <p>Code: <%= rsvp.code %>
          </p>
        </div>
        <% if current_user %>
          <div class="card-action">
            <% if rsvp.user_id == current_user.id %>
              <button class="event-unrsvp" data-event-id="<%= event.id %>" data-rsvp-id="<%= rsvp.id %>">UnRsvp</button>
            <% elsif @liked_user_id_arr.include?(rsvp.user_id) %>
              <% if like_id = @curr_user_liked_user_to_liked_id_hash[rsvp.user_id] %> 
                <button class="event-toggle-like-user user-likes" data-event-id="<%= event.id %>" data-like-id="<%= like_id %>">Unlike</button>
              <% else %>
               <!-- Could not get like_id for user -->
              <% end %>
            <% else %>
                <button class="event-toggle-like-user" data-event-id="<%= event.id %>" data-liked-user-id="<%= rsvp.user_id %>">Like</button>
            <% end %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
<% end %>